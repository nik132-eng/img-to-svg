{
  "name": "image-to-svg-converter",
  "version": "1.0.0",
  "description": "Full-stack Image to SVG Converter with React frontend and Fastify backend",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:backend": "npm run dev --workspace=backend",
    "build": "npm run build:backend && npm run build:frontend",
    "build:frontend": "npm run build --workspace=frontend",
    "build:backend": "npm run build --workspace=backend",
    "install:all": "npm install",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:frontend": "npm run preview --workspace=frontend",
    "start:backend": "npm start --workspace=backend",
    "clean": "rm -rf node_modules frontend/node_modules backend/node_modules",
    "clean:install": "npm run clean && npm install"
  },
  "keywords": [
    "svg",
    "converter",
    "react",
    "fastify",
    "vtracer",
    "fullstack",
    "monorepo"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
